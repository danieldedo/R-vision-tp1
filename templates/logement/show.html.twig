{% extends 'base.html.twig' %}

{% block title %}Logement{% endblock %}

{% block body %}
    <h1>Logement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ logement.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ logement.code }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ logement.nom }}</td>
            </tr>
            <tr>
                <th>Capacite</th>
                <td>{{ logement.capacite }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ logement.type }}</td>
            </tr>
            <tr>
                <th>Lieu</th>
                <td>{{ logement.lieu }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ logement.photo }}</td>
            </tr>
            <tr>
                <th>Disponibilite</th>
                <td>{{ logement.disponibilite ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_logement_index') }}">back to list</a>

    <a href="{{ path('app_logement_edit', {'id': logement.id}) }}">edit</a>

    {{ include('logement/_delete_form.html.twig') }}
{% endblock %}
